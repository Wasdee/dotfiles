# ~/.profile: executed by the command interpreter for login shells.
# This file is not read by bash(1), if ~/.bash_profile or ~/.bash_login
# exists.
# see /usr/share/doc/bash/examples/startup-files for examples.
# the files are located in the bash-doc package.

# the default umask is set in /etc/profile; for setting the umask
# for ssh logins, install and configure the libpam-umask package.
#umask 022

export RAN_DOTPROFILE=1

# if running bash
if [ -n "$BASH_VERSION" ]; then
    # include .bashrc if it exists
    if [ -f "$HOME/.bashrc" ]; then
        . "$HOME/.bashrc"
    fi
fi

# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/bin" ] && [ ":$PATH:" != *":$HOME/bin:"* ]; then
    PATH="$HOME/bin:$PATH"
fi

# set PATH so it includes user's private bin directories
export PNPM_HOME="$HOME/.local/share/pnpm"
if [ -d "$PNPM_HOME" ] && [ ":$PATH:" != *":$PNPM_HOME:"* ]; then
    PATH="$PNPM_HOME:$PATH"
fi
if [ -d "$PNPM_HOME" ] && [ ":$PATH:" != *":$PNPM_HOME:"* ]; then
    PATH="$PNPM_HOME:$PATH"
fi
if [ -d "$PNPM_HOME" ] && [ ":$PATH:" != *":$PNPM_HOME:"* ]; then
    PATH="$PNPM_HOME:$PATH"
fi

export PATH="$PATH:/snap/bin"
export PATH="$PATH:$HOME/.cargo/bin"


# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/.local/bin" ] && [ ":$PATH:" != *":$HOME/.local/bin:"* ]; then
    export PATH="$HOME/.local/bin:$PATH"
fi

# Created by `pipx` on 2023-07-29 09:36:41
PIPX_BIN_PATH="/home/nam/.local/bin"
if [ -d "$PIPX_BIN_PATH" ] && [ ":$PATH:" != *":$PIPX_BIN_PATH:"* ]; then
if [ -d "$PIPX_BIN_PATH" ] && [ ":$PATH:" != *":$PIPX_BIN_PATH:"* ]; then
    PATH="$PATH:$PIPX_BIN_PATH"
fi


export PATH="$HOME/.local/share/mise/shims:$PATH"

export FLYCTL_INSTALL="$HOME/.fly"
export PATH="$FLYCTL_INSTALL/bin:$PATH"

export PATH=/usr/bin:$PATH

# fix poetry keyring https://github.com/python-poetry/poetry/issues/1917
export PYTHON_KEYRING_BACKEND=keyring.backends.fail.Keyring
